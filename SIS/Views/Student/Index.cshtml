@using SIS.Models;
@model List<Student>

@{
    ViewBag.Title = "Index";
}

<h2>List of Students</h2>
<table class="table">
    <tr>
        <th>
            Id
        </th>
        <th>
            First Name
        </th>
        <th>
            Last Name
        </th>
        <th>
            Email
        </th>
    </tr>
    @foreach (var item in Model)
    {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FirstName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.LastName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Email)
        </td>
        <td>
            @Html.ActionLink("Edit", "EditStudent", new { })
            @Html.ActionLink("Details", "DetailsOfStudent", new { })
            @Html.ActionLink("Delete", "DeleteStudent", new { })
        </td>
    </tr>

    }
</table>

